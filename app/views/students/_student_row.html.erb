                <tr class="student-row">
                  <td class="name"><%= link_to presenter.full_name, student_url(presenter.id) %></td>
                  <td class="demographics"><%= presenter.home_language %></td>
                  <% star_result = presenter.latest_star %>
                  <% if star_result.present? %>
                    <% present(star_result) do |result_presenter| %>
                      <%= render "students/star_row_fragment", star_presenter: result_presenter %>
                    <% end %>
                  <% else %>
                    <%= render "students/blank_star_row" %>
                  <% end %>
                  <% mcas_result = presenter.latest_mcas %>
                  <% if mcas_result.present? %>
                    <% present(mcas_result) do |result_presenter| %>
                      <%= render "students/mcas_row_fragment", mcas_presenter: result_presenter %>
                    <% end %>
                  <% else %>
                    <%= render "students/blank_mcas_row" %>
                  <% end %>

                  <% if presenter.current_events.present? %>
                    <%= render "students/attendance_row_fragment", attendance_presenter: presenter %>
                    <%= render "students/discipline_row_fragment", discipline_presenter: presenter %>
                  <% else %>
                    <%= render "students/blank_attendance_row" %>
                    <%= render "students/blank_discipline_row" %>
                  <% end %>



                </tr>
