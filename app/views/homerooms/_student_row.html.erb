                <tr class="student-row">
                  <td class="name" href="<%= student_url(student) %>">
                    <div class="cell-wrapper">
                      <%= presenter.full_name %>
                    </div>
                  </td>
                  <td class="risk risk-tooltip-circle" data-student-id="<%= student.id %> "href="<%= student_url(student) %>">
                    <div class="warning-bubble <%= presenter.risk_level_css_class_name %>">
                      <%= presenter.risk_level_as_string %>
                    </div>
                  </td>
                  <td class="program" href="<%= student_url(student) %>">
                    <%= presenter.program_assigned %>
                  </td>
                  <td class="sped" href="<%= student_url(student) %>">
                    <%= presenter.disability %>
                  </td>
                  <td class="sped" href="<%= student_url(student) %>">
                    <%= presenter.sped_level_of_need %>
                  </td>
                  <td class="sped" href="<%= student_url(student) %>">
                    <%= presenter.plan_504 %>
                  </td>
                  <td class="language" href="<%= student_url(student) %>">
                    <%= presenter.limited_english_proficiency %>
                  </td>
                  <td class="language" href="<%= student_url(student) %>">
                    <%= presenter.home_language %>
                  </td>
                  <td class="free-reduced" href="<%= student_url(student) %>">
                    <%= presenter.free_reduced_lunch %>
                  </td>

                  <%= render "star_math_row_fragment", student: student, presenter: StarValuePresenter.new(assessment_data[:star_math_row_data]) %>

                  <%= render "star_reading_row_fragment", student: student, presenter: StarValuePresenter.new(assessment_data[:star_reading_row_data]) %>

                  <%= render "mcas_row_fragment", student: student, presenter: McasValuePresenter.new(assessment_data[:mcas_math_row_data]) %>

                  <%= render "mcas_row_fragment", student: student, presenter: McasValuePresenter.new(assessment_data[:mcas_ela_row_data]) %>

                  <%= render "access_row_fragment", student: student, presenter: GenericAssessmentValuePresenter.new(assessment_data[:access_row_data]) %>

                  <%= render "dibels_row_fragment", student: student, presenter: GenericAssessmentValuePresenter.new(assessment_data[:dibels_row_data]) %>

                </tr>
