                    <% if assessment.is_a? MissingStudentAssessment %>
                      <td class="map" href="<%= student_url(student, chart_start: "map-scaled") %>">
                        —
                      </td>
                      <td class="map" href="<%= student_url(student, chart_start: "map-growth") %>">
                      —
                      </td>
                    <% else %>
                      <% present(assessment) do |presenter| %>
                        <td class="map" href="<%= student_url(student, chart_start: "map-scaled") %>">
                            <% if risk.performance_warning? %>
                              <div class="warning-text"><%= presenter.performance_level %></div>
                            <% else %>
                              <%= presenter.performance_level %>
                            <% end %>
                        </td>
                        <td class="map" href="<%= student_url(student, chart_start: "map-growth") %>">
                            <% if risk.growth_warning? %>
                              <div class="warning-text"><%= presenter.growth_percentile %></div>
                            <% else %>
                              <%= presenter.growth_percentile %>
                            <% end %>
                        </td>
                      <% end %>
                    <% end %>
